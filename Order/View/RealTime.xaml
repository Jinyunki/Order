<UserControl x:Class="Order.View.RealTime"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Order.View"
             xmlns:model="clr-namespace:Order.Model"
             mc:Ignorable="d" Background="Green"
             d:DesignHeight="450" d:DesignWidth="450" 
             DataContext="{Binding Source={StaticResource Locator}, Path=SideBarViewModel}">
    <Grid>
        <TextBlock Text="{Binding Message,UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</UserControl>
